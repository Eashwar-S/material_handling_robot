<launch>
  <arg name="world_file"
       default="$(find material_handling_robot)/world/robotworld.world"/>
  <include file= "$(find turtlebot_gazebo)/launch/turtlebot_world.launch"> 
      <arg name="world_file" 
           value="$(arg world_file)"/>
  </include>
  <node name="pickupdropoff" 
        pkg="material_handling_robot" 
        type="pickupdropoff"/>
   
  <node name="vmarker" pkg="material_handling_robot" type="vmarker"/>

  
    <include file="$(find turtlebot_gazebo)/launch/amcl_demo.launch"> 
       <arg name="map_file" value="$(find material_handling_robot)/maps/my_map.yaml"/>
    </include>
    <include file="$(find turtlebot_rviz_launchers)/launch/view_navigation.launch">

    </include>
</launch>
